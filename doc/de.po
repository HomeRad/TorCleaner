#: en/webcleaner.conf.5:99
#, no-wrap
msgid "B<Rewriter>"
msgstr "B<Rewriter>"

# type: Plain text
#: en/webcleaner.conf.5:103
msgid ""
"Parse the HTML stream and replace arbitrary tag constructs; filters most "
"advertising."
msgstr ""
"Parst den HTML Datenstrom und ersetzt beliebe Tagkonstrukte. Filtert die "
"meiste Werbung heraus."

# type: TP
#: en/webcleaner.conf.5:103
#, no-wrap
msgid "B<VirusFilter>"
msgstr "B<VirusFilter>"

# type: Plain text
#: en/webcleaner.conf.5:106
msgid "Turns on clamav virus filtering (if clamav is installed)."
msgstr "Aktiviert den Clamav Virusfilter (falls Clamav installiert ist)."

