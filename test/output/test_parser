HTML '<a b="c">'
p starttag 'a' [('b', '"c"')]
HTML "<a b='c'>"
p starttag 'a' [('b', "'c'")]
HTML '<a b=c">'
p starttag 'a' [('b', 'c"')]
HTML '<a b="c>'
p starttag 'a' [('b', '"c')]
HTML '<a b=>'
p starttag 'a' [('b', '')]
HTML '<a =c>'
p starttag 'a' [('', 'c')]
HTML '<a =>'
p starttag 'a' [('', '')]
HTML '<a >'
p starttag 'a' []
HTML '< a>'
p starttag 'a' []
HTML '< a >'
