HTML '<!DOCTYPE adrbook SYSTEM "adrbook.dtd">'
p decl 'DOCTYPE' [('adrbook', None), ('SYSTEM', None), ('"adrbook.dtd"', None)]
HTML '<?xml version="1.0" encoding="latin1"?>'
p starttag '?xml' [('version', '1.0'), ('encoding', '"latin1"?')]
