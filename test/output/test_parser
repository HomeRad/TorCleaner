p comment ''
HTML '<!----->'
p comment '-'
HTML '<!------>'
p comment '--'
HTML '<!------->'
p comment '---'
HTML '<!---- >'
p comment ''
HTML '<!-- -->'
p comment ' '
HTML '<!-- -- >'
p comment ' '
HTML '<!---- />-->'
p comment '-- />'
HTML '</br>'
p endtag 'br'
HTML '</ br>'
p endtag 'br'
HTML '</ br >'
p endtag 'br'
HTML '</br >'
p endtag 'br'
HTML '< / br>'
