#! /bin/sh
# postrm script for webcleaner
#
# see: dh_installdeb(1)

set -e

case "$1" in
    purge)
       # remove log files
       rm -rf /var/log/webcleaner || true
       ;;
    remove|upgrade|failed-upgrade|abort-install|abort-upgrade|disappear)
       # nothing to do
       ;;
    *)
        echo "postrm called with unknown argument \`$1'" >&2
        exit 0

esac

#DEBHELPER#

exit 0
