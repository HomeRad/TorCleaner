#!/bin/sh
# prerm script for webcleaner
#
# see: dh_installdeb(1)
set -e

PACKAGE=webcleaner

#DEBHELPER#

# delete all generated .pyc and .pyo files
# do this _after_ debhelper due to the call to 
# /etc/init.d/webcleaner stop
dpkg --listfiles $PACKAGE |
        awk '$0~/\.py$/ {print $0"c\n" $0"o"}' |
        xargs rm -f >&2

exit 0
