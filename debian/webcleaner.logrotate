# Logrotate file for WebCleaner

/var/log/webcleaner/webcleaner-access.log /var/log/webcleaner/webcleaner.log /var/log/webcleaner.critical {
	compress
	weekly
	rotate 5
	notifempty
        prerotate
          /etc/init.d/webcleaner stop
        endscript
        postrotate
          /etc/init.d/webcleaner start
        endscript
}
